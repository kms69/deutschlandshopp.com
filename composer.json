{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/blog": "^3.0",
        "drupal/bootstrap": "^3.23",
        "drupal/captcha": "1.x-dev@dev",
        "drupal/commerce": "^2.25",
        "drupal/commerce_product_taxonomy_filter": "1.0.x-dev@dev",
        "drupal/commerce_shipping": "^2.1",
        "drupal/commerce_statistic": "1.0.x-dev@dev",
        "drupal/commerce_wishlist": "3.x-dev@dev",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-project-message": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/estore": "^1.14",
        "drupal/honeypot": "^2.0",
        "drupal/inline_entity_form": "1.x-dev@dev",
        "drupal/markup": "1.x-dev@dev",
        "drupal/page_manager": "4.x-dev@dev",
        "drupal/phpmailer_smtp": "^2.0",
        "drupal/quicktabs": "3.x-dev@dev",
        "drupal/recaptcha": "^3.0",
        "drupal/sendgrid_integration": "^1.7",
        "drupal/shs": "1.x-dev@dev",
        "drupal/sshop": "^1.12",
        "drupal/superfish": "^1.4",
        "drupal/svg_image": "^1.14",
        "drupal/swiftmailer": "2.x-dev@dev",
        "drupal/taxonomy_menu": "3.x-dev@dev",
        "drupal/views_bootstrap": "4.x-dev@dev",
        "drupal/views_list_sort": "^1.4",
        "drupal/views_taxonomy_term_name_depth": "^7.0",
        "drush/drush": "^10.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {


       "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
